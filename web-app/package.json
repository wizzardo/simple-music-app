{
  "private": true,
  "name": "ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build-css": "sass --no-source-map src:src/",
    "watch-css": "npm run build-css && sass --no-source-map --watch src:src",
    "clean": "rm -rf build/* && rm -rf buildts/*",
    "build": "preact build --no-prerender --no-sw",
    "build-rollup": "NODE_ENV=production rollup -c rollup.config.js",
    "build-ts": "NODE_ENV=production rollup -c rollup.config-process-ts.js",
    "serve": "sirv build --port 3000 --cors --single",
    "start-js": "preact watch -p 3000 --no-sw",
    "start": "npm-run-all -p watch-css start-js",
    "gzip-build": "gzip --keep --best -r build/static/*/*.js && gzip --keep --best -r build/static/*/*.css",
    "dist": "npm-run-all clean build-css build-ts replace-react build-rollup gzip-build copy-build",
    "dev": "preact watch",
    "copy-build": "rm -rf ../be/src/main/resources/public/* && cp -r build/* ../be/src/main/resources/public/",
    "replace-react": "node replace-react.js",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-external-helpers": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@babel/plugin-proposal-do-expressions": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-function-bind": "^7.16.7",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.17.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.16",
    "babel-plugin-jsx-pragmatic": "1.0.2",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^3.4.0",
    "eslint": "^8.12.0",
    "eslint-config-preact": "^1.3.0",
    "fs-extra": "^10.0.1",
    "jest": "^27.5.1",
    "jest-preset-preact": "^4.0.5",
    "npm-run-all": "4.1.5",
    "preact-cli": "^3.3.5",
    "rollup": "2.70.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.9",
    "sirv-cli": "2.0.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "dayjs": "^1.11.1",
    "goober": "^2.1.9",
    "preact": "^10.6.6",
    "preact-render-to-string": "^5.1.20",
    "preact-router": "^4.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-ui-basics": "0.0.170"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
